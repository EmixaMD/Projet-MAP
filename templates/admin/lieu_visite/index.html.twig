{% extends 'base.html.twig' %}

{% block title %}LieuVisite index{% endblock %}

{% block body %}
    <h1>LieuVisite index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for lieu_visite in lieu_visites %}
            <tr>
                <td>{{ lieu_visite.id }}</td>
                <td>{{ lieu_visite.nom }}</td>
                <td>
                    <a href="{{ path('admin_lieu_visite_show', {'id': lieu_visite.id}) }}">show</a>
                    <a href="{{ path('admin_lieu_visite_edit', {'id': lieu_visite.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('admin_lieu_visite_new') }}">Create new</a>
{% endblock %}
